doctype html
html
  head
    title= title
    meta(name="renderer",content="webkit")
    meta(http-equiv="X-UA-Compatible",content="IE=edge")
    meta(name="viewport" content="width=device-width, initial-scale=1")
    style!= style
  body
    main.container
      block header
      div.page-header
        h1=title
        small(style="margin-left:10px;")=subtitle
      block content
